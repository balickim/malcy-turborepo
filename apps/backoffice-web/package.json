{
  "name": "backoffice-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --no-lint",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:android": "npm run build && npx cap sync && npx cap run android",
    "start:ios": "npm run build && npx cap sync && npx cap run ios",
    "open:android": "npm run build && npx cap sync && npx cap copy android && npx cap open android",
    "open:ios": "npm run build && npx cap sync && npx cap copy ios && npx cap open ios",
    "generate:icons": "capacitor-assets generate",
    "test.e2e": "cypress run",
    "test.unit": "vitest"
  },
  "dependencies": {
    "ui": "workspace:*",
    "shared-types": "workspace:*",
    "@capacitor/android": "^5.7.0",
    "@capacitor/app": "5.0.7",
    "@capacitor/core": "5.7.0",
    "@capacitor/geolocation": "^5.0.7",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "^5.7.0",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "5.0.7",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.25",
    "ionicons": "^7.0.0",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "mobx": "^6.12.0",
    "mobx-persist-store": "^1.1.4",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hot-toast": "^2.4.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "socket.io-client": "^4.7.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-swc": "^3.3.2",
